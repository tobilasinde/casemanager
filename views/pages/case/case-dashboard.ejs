    <div class="col-lg-12 col-xl-8 order-lg-2 order-xl-1">
        <div class="kt-container  kt-container--fluid  kt-grid__item kt-grid__item--fluid">
            <div class="kt-portlet kt-portlet--mobile">
                <div class="kt-portlet__head">
                    <div class="kt-portlet__head-label">
                        <h3 class="kt-portlet__head-title">
                            <%= title %>
                        </h3>
                    </div>
                    <div class="kt-portlet__head-toolbar">
                        <div class="kt-portlet__head-toolbar">
                            <div class="dropdown dropdown-inline">
                                <a href="/case/create" class="btn btn-clean btn-sm btn-icon btn-icon-md" aria-haspopup="true" aria-expanded="false">
                                    <i class="la la-pencil"></i>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="kt-portlet__body">
        
                    <!--begin: Datatable -->
                    <table class="table table-striped- table-bordered table-hover table-checkable" id="kt_table_1">
                        <thead>
                            <tr>
                                <th>Case Number</th>
                                <th>Subject</th>
                                <th>Priority</th>
                                <th>Status</th>
                                <th>Actions</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% casemanagers.forEach(function(casemanager) { %>
                                <tr>
                                    <td><%= casemanager.case_number %></td>
                                    <td><%= casemanager.subject %></td>
                                    <td><%= casemanager.priority %></td>
                                    <td><%= casemanager.status %></td>
                                    <td>
                                        <span>
                                            <a href="/case/<%= casemanager.id %>/details" class="btn btn-sm btn-clean btn-icon btn-icon-md" title="View">
                                                <i class="la la-eye"></i>
                                            </a>
                                        </span>
                                    </td>
                                </tr>
                            <% }); %>
                        </tbody>
                    </table>
        
                    <!--end: Datatable -->
                </div>
            </div>
        </div>
